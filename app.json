{
  "expo": {
    "name": "Gymless",
    "slug": "gymless",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymless",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Gymless uses your camera to track exercise movements, count reps, and provide real-time form feedback."
      }
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "package": "com.theatomicshift.gymless"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "77554980-81fe-466f-b880-5d7c16fc00de"
      }
    }
  }
}
